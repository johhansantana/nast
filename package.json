{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "postcss": "postcss --config postcss.config.js --replace ./static/css/app.css",
    "css:prod": "npm run gulp && npm run postcss",
    "dev": "concurrently \"tsc --watch\" \"node server.babel.js -p $PORT\"",
    "build": "npm run css:prod && next build",
    "start": "NODE_ENV=production node server.babel.js -p $PORT",
    "heroku-build": "next build",
    "export": "next export",
    "prod": "npm run css:prod && npm run heroku-build",
    "heroku-postbuild": "npm run prod"
  },
  "dependencies": {
    "express": "^4.15.4",
    "express-logging": "^1.1.1",
    "logops": "^2.1.0",
    "next": "^4.1.4",
    "react": "^16.2.0",
    "react-apollo": "^1.4.11",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^8.0.20",
    "@types/react": "^15.0.1",
    "autoprefixer": "^7.1.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "concurrently": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "postcss-cli": "^4.1.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "typescript": "^2.1.5"
  }
}
